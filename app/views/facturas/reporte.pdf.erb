      <tbody>
        <% @facturas.each do |factura| %>
        <tr>
          <td> <%= factura.user %></td>
          <td> <%= factura.client %></td>
          <td> <%= factura.product %></td>
          <td> <%= factura.service %></td>
          <td> <%= factura.fecha %></td>
          <td> <%= factura.price %></td>
          <td> <%= factura.details %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
